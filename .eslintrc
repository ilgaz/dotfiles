{
	"extends": [
		"airbnb",
		"prettier"
	],
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"rules": {
		"for-direction": "error",
		"no-compare-neg-zero": 2,
		"no-cond-assign": 2,
		"no-constant-condition": "warn",
		"no-empty": 2,
		"no-func-assign": "error",
		"no-irregular-whitespace": "error",
		"no-template-curly-in-string": "error",
		"no-unreachable": "error",
		"array-callback-return": "error",
		"block-scoped-var": "error",
		"dot-notation": "error",
		"eqeqeq": [
			"error",
			"smart"
		],
		"no-alert": "error",
		"no-else-return": [
			"error",
			{
				"allowElseIf": false
			}
		],
		"no-empty-function": "error",
		"no-eq-null": "error",
		"no-multi-spaces": [
			"error",
			{
				"ignoreEOLComments": false
			}
		],
		"no-self-assign": "error",
		"no-self-compare": "error",
		"no-useless-catch": "error",
		"no-useless-concat": "error",
		"no-useless-return": "error",
		"require-await": "error",
		"no-delete-var": "error",
		"no-shadow": [
			"error",
			{
				"builtinGlobals": true
			}
		],
		"no-unused-vars": "error",
		"global-require": "error",
		"array-bracket-newline": [
			"error",
			{
				"multiline": true
			}
		],
		"array-bracket-spacing": [
			"error",
			"always"
		],
		"block-spacing": "error",
		"brace-style": "error",
		"comma-dangle": [
			"error",
			"never"
		],
		"comma-style": "error",
		"no-lonely-if": "error",
		"no-multiple-empty-lines": "error",
		"object-curly-newline": [
			"error",
			{
				"multiline": true
			}
		],
		"quotes": [
			"error",
			"single"
		],
		"space-in-parens": [
			"error",
			"always"
		],
		"arrow-body-style": [
			"error",
			"as-needed"
		],
		"arrow-parens": [
			"error",
			"as-needed"
		],
		"arrow-spacing": "error",
		"no-const-assign": "error"
	}
}
